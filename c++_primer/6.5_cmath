#include <iostream>
#include <cmath>
#include <vector>
#include <cstdlib>
#include <algorithm>
#include <cstring>

int myabs(int a)
{
    return abs(a);
}

int main(){
    using namespace std;
    int val = 0;
    cout << "Please input a value: ";
    cin >> val;
    int valFact = myabs(val);
    cout << "The absolute of " << val << " is: " << valFact;
    
    return 0;
}
