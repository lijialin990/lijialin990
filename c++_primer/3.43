#include <iostream>
#include <cstring>
#include <cstdlib>
#include <ctime>
#include <algorithm>
#include <vector>
int main() {
    using namespace std;

    int ia[3][4] = {0};
    //cout << size(ia);

    for(int (&i)[4] : ia)
    {
        for(int &j : i)
        {
            cout << j << " ";
        }
        cout << endl;
    }

    for(int i = 0; i < 3; i++)
    {
        for(int j = 0; j < 4; j++)
            cout << ia[i][j] << " ";
        cout << endl;
    }

    for(int (*p1)[4] = begin(ia); p1 != end(ia); p1++)
    {
        for(int * p2 = begin(*p1); p2 != *p1 + 4; p2++)
            cout << *p2 << " ";
        cout << endl;
    }
